{
  "name": "proxia",
  "version": "0.0.1",
  "description": "A Proxy Messaging Bot",
  "repository": "https://github.com/cloudytraveller/Proxia.git",
  "author": "Cloudy",
  "license": "zlib",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/index.ts",
    "lint": "cross-env eslint --fix .",
    "format": "cross-env prettier --write './**/**/*.{js,json,md,ts,yml}'",
    "prebuild": "cross-env rimraf ./dist/",
    "start": "pnpm build && cross-env NODE_ENV=production node --no-warnings ./dist/src/index.js",
    "test": "cross-env tsc --noEmit && eslint . && prettier --c ."
  },
  "engines": {
    "node": "^18.12.1",
    "pnpm": ">=7.0.0"
  },
  "dependencies": {
    "discord-api-types": "^0.37.18",
    "axios": "^1.1.3",
    "better-sqlite3": "^7.6.2",
    "discord.js": "^14.6.0",
    "knex": "^2.3.0",
    "nodemon": "^2.0.20",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/eslint": "^8.4.10",
    "@typescript-eslint/eslint-plugin": ">=5.43.0",
    "@typescript-eslint/parser": ">=5.43.0",
    "@sysdotini/eslint-config": "^3.1.2",
    "eslint": ">=8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": ">=4.4"
  }
}
